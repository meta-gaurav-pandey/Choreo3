<?xml version="1.0" encoding="UTF-8"?>
<sequence name="TTlDatProcessSequence" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log>
        <property name="Start Logger" value="TTLDataSequenceStarted"/>
    </log>
    <call>
        <endpoint>
            <http method="post" uri-template="http://localhost:8081/insertDataIntoGraph">
                <suspendOnFailure>
                    <initialDuration>-1</initialDuration>
                    <progressionFactor>-1</progressionFactor>
                    <maximumDuration>0</maximumDuration>
                </suspendOnFailure>
                <markForSuspension>
                    <retriesBeforeSuspension>0</retriesBeforeSuspension>
                </markForSuspension>
            </http>
        </endpoint>
    </call>
    <log>
        <property name="End Logger" value="TTL Data Sequence Ended"/>
    </log>
</sequence>
